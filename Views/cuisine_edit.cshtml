@{
  Layout="Shared/_Layout.cshtml";
}


<h1>Cuisines</h1>
<hr />
<h2>Edit cuisine: @Model.GetCuisineType()</h2>
<form action="/cuisine/edit/@Model.GetId()" method="post">
  <input type="hidden" name="_method" value="PATCH">
  <input name="cuisine-id" type="hidden" value="@Model.GetId()">
  <label for="cuisine-type">Category type</label>
  <input id="cuisine-type" name="cuisine-type" type="text">
  <button type="submit">Submit</button>
</form>
<h5><a href="/">Return home</a></h5>
